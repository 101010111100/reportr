{
    "manifest_version": 2,

    "name": "Reportr Tracker",
    "description": "This extension demonstrates how to track browser navigation with Reportr.",
    "version": "1.0",

    "permissions": [
        "history"
    ],
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "background": {
        "scripts": ["tracker.js"],
        "persistent": true
    }
}